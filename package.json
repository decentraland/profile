    "react-dom": "^17.0.2",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.14.0",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@dcl/eslint-config": "1.1.7",
    "@testing-library/react": "^14.0.0",
