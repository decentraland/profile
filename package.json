    "decentraland-ui": "^4.0.0",
    "p-queue": "^6.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.14.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.3"
