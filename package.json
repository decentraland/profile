    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
